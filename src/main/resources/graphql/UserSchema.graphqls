input UserCreateInput {
    apnToken: String
    balance: Float
    fcmToken: String
    firstName: String
    isActive: Boolean
    isDebtor: Boolean
    lastName: String
    onBoardingStatus: Int
    password: String
    phone: String
    ratings: Float
    totalRatings: Int
    roles: [RoleCreateInput]
    userProfile: UserProfileCreateInput
    translatorProfile: TranslatorProfileCreateInput
}
input UserUpdateInput {
    id: ID!
    apnToken: String
    balance: Float
    fcmToken: String
    firstName: String
    isActive: Boolean
    isDebtor: Boolean
    lastName: String
    onBoardingStatus: Int
    password: String
    phone: String
    ratings: Float
    totalRatings: Int
    roles: [RoleUpdateInput]
    userProfile: UserProfileUpdateInput
    translatorProfile: TranslatorProfileUpdateInput
}

input UserRolesUpdateInput{
    userId: ID!
    roles: [RoleCreateInput]!
}

type User {
    id: ID
    apnToken: String
    balance: Float
    fcmToken: String
    firstName: String
    isActive: Boolean
    isDebtor: Boolean
    lastName: String
    onBoardingStatus: Int
    password: String
    phone: String
    ratings: Float
    totalRatings: Int
    roles: [Role]
    userProfile: UserProfile
    translatorProfile: TranslatorProfile
}
