input RegistrationUserCreateInput {
    phone: String!
    password: String!
    roles: [RoleCreateInput]!
}

input LoginUserInput {
    phone: String!
    password: String!
}

input RefreshTokenInput {
    refreshToken: String!
}

type LoginPayload {
    user: User
    token: String
    refreshToken: String
}

type RegistrationPayload {
    user: User
    refreshToken: String
}

type RefreshTokenPayload {
    accessToken: String
    refreshToken: String
}



