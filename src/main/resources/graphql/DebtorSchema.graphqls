input DebtorCreateInput {
    accountHolder: String
    isPaid: Boolean
    nameOfBank: String
    user: UserCreateInput
}

input DebtorUpdateInput {
    id: ID!
    accountHolder: String
    isPaid: Boolean
    nameOfBank: String
    user: UserUpdateInput
}

type Debtor {
    id: ID
    accountHolder: String
    isPaid: Boolean
    nameOfBank: String
    user: User
}
