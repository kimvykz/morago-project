input NotificationCreateInput {
    platform: EPlatform!
    date: String #DateTime
    text: String
    time: String #DateTime
    title: String
    user: UserCreateInput
}
input NotificationUpdateInput {
    id: ID!
    platform: EPlatform!
    date: String #DateTime
    text: String
    time: String #DateTime
    title: String
    user: UserUpdateInput
}
type Notification {
    id: ID
    date: String #DateTime
    text: String
    time: String #DateTime
    title: String
    user: User
}

